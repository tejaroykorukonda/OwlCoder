#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    if(n%2==0 & n%7==0)
	    {
	        cout<<"Alice"<<"\n";
	    }
	    else if(n%2!=0 & n%9==0)
	    {
	        cout<<"Bob"<<"\n";
	    }
	    else
	    {
	        cout<<"Charlie"<<"\n";
	    }
	}

}

Candy Store
----------------------

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n,m;
	    cin>>n>>m;
	    if(n>m)
	    {
	        cout<<m<<"\n";
	    }
	    else
	    {
	        cout<<(n+(m-n)*2)<<"\n";
	    }
	}

}
